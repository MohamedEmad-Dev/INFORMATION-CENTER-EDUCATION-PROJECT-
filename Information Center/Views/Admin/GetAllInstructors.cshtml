@model IEnumerable<Information_Center.Models.ApplicationUser>
@{
    ViewBag.Title = "GetAllInstructors";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="recent-grid">
    <div class="container">
        <div class="row">

            <div class="projects">
                <div class="card">
                    <div class="card-header">
                        <h2>المعلمين</h2>
                        <button>للمزيد <i class="fa fa-arrow-right"></i></button>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table width="100%">
                                <thead>
                                    <tr>
                                        <td>@Html.DisplayNameFor(model => model.Email)</td>
                                        <td>@Html.DisplayNameFor(model => model.UserName)</td>
                                        <td>@Html.DisplayNameFor(model => model.UserType)</td>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var user in Model)
                                    {
                                        <tr>
                                            <td>@user.Email</td>
                                            <td>@user.Name</td>
                                            <td>@user.UserType</td>
                                        </tr>
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

